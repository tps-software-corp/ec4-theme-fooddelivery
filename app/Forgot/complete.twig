{% extends 'default_frame.twig' %}

{% block main %}
    <div class="ec-role">
        <div class="ec-pageHeader">
            <h1>{{ 'front.forgot.complete_title'|trans }}</h1>
        </div>
    </div>
    <div class="ec-forgetCompleteRole">
        <div class="ec-off3Grid">
            <div class="ec-role">
                <div class="col-xs-10 col-xs-12 col-xs-offset-1 col-xs-offset-0">
                    <div class="ec-reportHeading">
                        <h2>{{ 'front.forgot.complete_message__title'|trans }}</h2>
                    </div>
                    <p class="ec-reportDescription">{{ 'front.forgot.complete_message__body'|trans|nl2br }}</p>
                </div>
            </div>
        </div>
    </div>
{% endblock %}


